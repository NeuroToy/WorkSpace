<greenhouse>
	<plant name="poppy" soil="black earth" origin="NL">
		<visualParameters>
			<color>red</color>
		</visualParameters>
		<growingTips>
			<temperature unit="degrees">17</temperature>
			<watering>600</watering>
		</growingTips>
		<multiplying>
			<reproduction>seed</reproduction>
		</multiplying>
	</plant>
	<plant name="maple" soil="garden" origin="CA">
		<visualParameters>
			<color>green</color>
		</visualParameters>
		<growingTips>
			<temperature unit="degrees">12</temperature>
			<watering>1200</watering>
		</growingTips>
		<multiplying>
			<reproduction>stalk</reproduction>
		</multiplying>
	</plant>
</greenhouse>
